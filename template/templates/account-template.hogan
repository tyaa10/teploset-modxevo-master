	        <label for="tbl-2">
	            [%Сверка%]
	        </label>
	        <input id="tbl-3" name="user-menu-tbl" type="radio"/>
	        <label for="tbl-3">
	            [%Квитанция%]
	        </label> -->
            <input id="tbl-4" name="user-menu-tbl" type="radio"/>
            <label for="tbl-4">
                {{dict.Договор}}
            </label>
            <input id="tbl-5" name="user-menu-tbl" type="radio"/>
            <label for="tbl-5">
                {{dict.Платежи}}
            </label>
            <input id="tbl-6" name="user-menu-tbl" type="radio"/>
            <label for="tbl-6">
                {{dict.Субсидии}}
            </label>
            <input id="tbl-7" name="user-menu-tbl" type="radio"/>
            <label for="tbl-7">
                Оплата
            </label>
            <input id="tbl-8" name="user-menu-tbl" type="radio"/>
            <label for="tbl-8">
                {{dict.Приборы учета}}
            </label>
            <input id="tbl-9" name="user-menu-tbl" type="radio"/>
            <label for="tbl-9">
                {{dict.Профиль}}
            </label>
            <input id="tbl-10" name="user-menu-tbl" type="radio"/>
            <label for="tbl-10">
                {{dict.Интернет приемная}}
            </label>
            <div id="user-show">
                <div>
                    <h3>
                        {{dict.Информация}}
                    </h3>
                    <div class="table" id="account-info">
                        <div>
                            <span>
                                {{dict.лицевой счет}}: {{accountDetails.code}}
                            </span>
                        </div>
                        <div>
                            <span>
                                {{dict.адрес}}: {{accountDetails.address}}
                            </span>
                        </div>
                        <div>
                            <span>
                                {{dict.основной квартиросъемщик}}: {{accountDetails.fio}}
                            </span>
                        </div>
                        <div>
                            <span>
                                {{dict.телефоны абонента}}: {{accountDetails.phone}}
                            </span>
                        </div>
                        <div>
                            <span>
                                {{dict.цтп}}: {{accountDetails.heatpoint}}
                            </span>
                        </div>
                        <div>
                            <span>
                                {{dict.район}}: {{accountDetails.area}}
                            </span>
                        </div>
                        <div>
                            <span>
                                {{dict.котельная}}: {{accountDetails.boiler}}
                            </span>
                        </div>
                        <div>
                            <span>
                                {{dict.адрес и телефон абонентного участка}}: {{accountDetails.areaAddress}}
                            </span>
                        </div>
                    </div>
                </div>
                <div>
                    <h3>
                        Сверка
                    </h3>
                    <div class="table">
                        <!-- [[-TvTable? &tvname;=`tableRevise`]] -->
                        <div class="divTable">
                            <div class="divTableBody">
                                <div class="divTableRow">
                                	<div class="divTableCell">
                                        {{dict.Месяц}}
                                    </div>
                                    <div class="divTableCell">
                                        долг <br/>на первое число
                                    </div>
                                    <div class="divTableCell">
                                        горячая вода<br/>начислено
                                    </div>
                                    <div class="divTableCell">
                                        горячая вода<br/>оплата
                                    </div>
                                    <div class="divTableCell">
                                        тепло<br/>начислено
                                    </div>
                                    <div class="divTableCell">
                                        тепло<br/>оплата
                                    </div>
                                    <div class="divTableCell">
                                        сальдо
                                    </div>
                                </div>
                                {{#accountRevises}}
                                <div class="divTableRow">
                                    <div class="divTableCell">
                                        {{monthYear}}
                                    </div>
                                    <div class="divTableCell">
                                        {{beginningDebt}}
                                    </div>
                                    <div class="divTableCell">
                                        {{waterInvoice}}
                                    </div>
                                    <div class="divTableCell">
                                        {{waterPayment}}
                                    </div>
                                    <div class="divTableCell">
                                        {{heatInvoice}}
                                    </div>
                                    <div class="divTableCell">
                                        {{heatPayment}}
                                    </div>
                                    <div class="divTableCell">
                                        {{endDebt}}
                                    </div>
                                </div>
                                {{/accountRevises}}
	                    		{{^accountRevises}}
                                <div class="divTableRow">
                                    У Вас не было сверок
                                </div>
                                {{/accountRevises}}
                            </div>
                        </div>
                    </div>
                </div>
                <div>
                    <h3>
                        [%Квитанция%]
