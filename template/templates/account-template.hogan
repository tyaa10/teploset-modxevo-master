<div>
    <div id="account-template">
        <div class="section-main">
            <a class="btn-logout" href="#" id="logout">
                {{dict.Выход}}
            </a>
        </div>
        <div class="section-main" id="user-box">
            <input checked="" id="tbl-1" name="user-menu-tbl" type="radio"/>
            <label for="tbl-1">
                {{dict.Информация}}
            </label>
            <input id="tbl-2" name="user-menu-tbl" type="radio"/>
	        <label for="tbl-2">
	            Сверка
	        </label>
